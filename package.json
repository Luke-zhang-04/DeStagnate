    "jsdom": "^16.3.0",
    "mocha": "^8.0.1",
    "nice-try": "^2.0.1",
    "nyc": "^15.1.0",
    "typedoc": "^0.19.1",
    "typedoc-dark-theme": "^0.0.11",
