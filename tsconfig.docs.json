{
    "extends": "./tsconfig.json",
    "compilerOptions": {
        "jsx": "react",
        "jsxFactory": "DeStagnate.createElement",
        "allowJs": true,
        "checkJs": false,
        "outDir": "./docs/lib",
        "target": "ES5",
        "declaration": false,
        "noEmitOnError": false,
        "skipLibCheck": true,
    },
    "include": ["docs/src/", "docs/*.ts", "docs/*.tsx"],
    "exclude": ["./src/", "./lib"]
}
